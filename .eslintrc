{
  "env": {
    "node": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "plugin:jest/recommended",
    "plugin:security-node/recommended"
  ],
  "plugins": ["jest", "security-node"],
  "parserOptions": {
    "project": "./tsconfig.eslint.json"
  },
  "ignorePatterns": ["**/*.js"],
  "rules": {
    "@typescript-eslint/no-use-before-define": 0,
    "security-node/detect-unhandled-async-errors": 0,
    "class-methods-use-this": 0,
    "max-classes-per-file": 0,
    "import/prefer-default-export": 0,
    "no-prototype-builtins": 0,
    "global-require": 0,
    "arrow-body-style": 0,
    "import/order": 0,
    "consistent-return": 0,
    "func-names": 0,
    "prefer-destructuring": 0,
    "newline-per-chained-call": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "no-console": 0,
    "arrow-parens": 0,
    "no-else-return": 0,
    "radix": 0
  }
}
